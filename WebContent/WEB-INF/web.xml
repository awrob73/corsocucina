<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<display-name>ProgettoFineCorsoScuolaCucina</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<description></description>
		<display-name>ServletRegistrazioneUtente</display-name>
		<servlet-name>ServletRegistrazioneUtente</servlet-name>
		<servlet-class>it.ats.progettofinecorsoscuolacucina.controllo.ServletRegistrazioneUtente</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ServletRegistrazioneUtente</servlet-name>
		<url-pattern>/ServletRegistrazioneUtente</url-pattern>
	</servlet-mapping>

	<error-page>
		<exception-type>javax.servlet.ServletException</exception-type>
		<location>/error.jsp</location>
	</error-page>
</web-app>